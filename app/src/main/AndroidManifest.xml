<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aupadhyay.moduletwo">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CheckAndRadio" />
        <activity android:name=".FrameActivity" />
        <activity android:name=".TableActivity" />
        <activity android:name=".WebViewActivity" />
        <activity android:name=".SpinnerActivity" />
        <activity android:name=".AutoCompleteActivity" />
        <activity android:name=".DatePickerActivity" />
        <activity android:name=".TimePickerActivity" />
        <activity android:name=".NotificationsActivity" />
        <activity android:name=".FragmentActivity"></activity>
    </application>

</manifest>